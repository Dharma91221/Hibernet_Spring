<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="emp" class="com.beanFac.Employee">
<property name="empId" value="1234"></property>
<property name="name" value="anand"></property>  
<property name="hobbies"> 
<list>
	<value>Walking</value>
	<value>playing</value>
	<value>eating</value>
</list>
</property> 

<property name="email">
<list>
	<value>av@g.com</value>
	<value>fgh@g.com</value>
	<value>fgh@g.com</value>
</list>
</property> 

<property name="phn">
<set>
	<value>1234567890</value>
	<value>0987654321</value>
	<value>9885947561</value>
</set>
</property>

<property name="education">
<map>
<entry key="1" value="ECE"></entry>
<entry key="2" value="CSE"></entry>
</map>
</property>
</bean>	
</beans>